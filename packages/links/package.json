{
  "name": "@sill/links",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "development": {
        "types": "./src/index.ts",
        "import": "./src/index.ts"
      },
      "production": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "dev:local": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@atproto/api": "^0.18.17",
    "@atproto/oauth-client-node": "^0.2.5",
    "@aws-sdk/client-s3": "^3.864.0",
    "@sill/schema": "workspace:*",
    "@sill/auth": "workspace:*",
    "@sill/emails": "workspace:*",
    "cloudflare": "^4.4.1",
    "drizzle-orm": "^0.44.4",
    "masto": "^7.4.0",
    "open-graph-scraper-lite": "^2.1.0",
    "uuidv7-js": "^1.1.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "typescript": "^5.1.6",
    "vitest": "^4.0.17"
  }
}